<!--index.wxml-->
<view class="container">
  <!-- 用户信息 -->
  <view class="weui-cells weui-cells_after-title">
    <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__hd" style="height: 50px;">
        <image class="userinfo-avatar" src="{{ userInfo.avatarUrl }}"></image>
      </view>
      <view class="weui-cell__bd">{{ userInfo.nickName }}</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
  </view>
  <!-- 其余设置 -->
  <view class="weui-cells weui-cells_after-title">
    <navigator url="/pages/classManage/classManage" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__hd">
        <image src="{{icon}}" style="margin-right: 16px;vertical-align: middle;width:20px; height: 20px;"></image>
      </view>
      <view class="weui-cell__bd">分类管理</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
    <navigator url="" class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__hd">
        <image src="{{icon}}" style="margin-right: 16px;vertical-align: middle;width:20px; height: 20px;"></image>
      </view>
      <view class="weui-cell__bd">cell standard</view>
      <view class="weui-cell__ft weui-cell__ft_in-access">说明文字</view>
    </navigator>
  </view>
  <!-- 关于 -->
  <view class="weui-cells weui-cells_after-title">
    <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
      <view class="weui-cell__hd">
        <image src="{{icon}}" style="margin-right: 16px;vertical-align: middle;width:20px; height: 20px;"></image>
      </view>
      <view class="weui-cell__bd">关于</view>
      <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
  </view>
  <!-- 退出登录 -->
  <a class="weui-btn_cell weui-btn_cell-default">退出登录</a>
  <!-- <button class="sign-out-button" hover-class="weui-cell_active" style="width: 100%;">
    退出登录
  </button> -->
</view>